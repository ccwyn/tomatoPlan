.container {
    min-height: 100vh;

}

.log {
    color: #333;
    /* margin: 15px 10px; */
}

.log_item {
    padding: 15px 5px;
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 32rpx;
    border-bottom: 1px solid #f3f3f3 ;
}

.log_item text {
    padding-right: 20px;
    /* float: left; */
}

.clear {
    position: absolute;
    bottom: 0px;
    width: 100%;
}

.clear .clear_btn {
    margin: 10px;
    height: 35px;
    line-height: 35px;
    color: #fff;
    background-color: #fd8f58;
    border-radius: 2px;
}

.panel{
    box-sizing: border-box;
    height: 100%;

}